import { create_symbolic_link } from "std/fs"
main {
    unsafe $touch /tmp/amber-symbolic$
    if create_symbolic_link("/tmp/amber-symbolic", "/tmp/amber-symbolic-link") {
        echo "created"
    } else {
        echo "failed"
    }
    unsafe $rm /tmp/amber-symbolic$
    unsafe $rm /tmp/amber-symbolic-link$
} 
